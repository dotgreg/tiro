  },
  "author": "Apache Cordova Team",
  "license": "Apache-2.0",
  "dependencies": {
    "axios": "^0.21.1",
    "cordova-android": "^6.3.0",
    "cordova-browser": "^5.0.4",
    "cordova-plugin-console": "^1.1.0",
    "cordova-plugin-whitelist": "^1.3.4",
    "nodejs-mobile-cordova": "^0.4.2"
  },
  "cordova": {
    "plugins": {
      "cordova-plugin-whitelist": {},
