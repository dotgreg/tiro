    "test-apk": "npm i & cd platforms/android & cordova run android --emulator",
    "emulator-detached": "cd scripts & open-emulator.bat",
    "emulator": "C:\\Users\\grego\\AppData\\Local\\Android\\Sdk\\emulator\\emulator -avd Pixel_2_API_29  -no-boot-anim -no-snapshot-load",
